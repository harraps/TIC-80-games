-- title:  TIC-80 CHESS
-- author: Olivier Schyns
-- desc:   class-less Chess game
-- script: moonscript
-- input:  mouse
-- pal:    DB16

useMouse=true

--position:
--	000111 : X-coordinate
--	111000 : Y-coordinate

--piece:
--	00111 : type
--	01000 : team
--	10000 : moved

--evaluation of chess piece:
--	pawn   : 1
--	rook   : 4
--	knight : 3
--	bishop : 2
--	queen  : 5
--	king   : 6

--moves:
--	pass :
--		1 : destination of current pawn
--		2 : pos of enemy pawn to destroy
--	cast :
--		1 : destination of current king
--		2 : pos of rook to move
--		3 : destination of rook


mkBoard=->
	a={4,3,2,5,6,2,3,4}
	b={}
	for i=0,7
		b[i   ]=a[i+1]
		b[i+ 8]=1
		b[i+48]=9
		b[i+56]=a[i+1]+8
	return b

--add int to set
--coordinates from index
--copy array
--check if piece is of enemy team
--get moved flag
add =(t,e)-> t[#t+1]=e
pos =(id) -> id%8,id//8
copy=(t)  -> {k,v for k,v in pairs t}
enm =(p,c)-> (p%16>=8)~=c
mvd =(p)  -> p+(p<16 and 16 or 0)

--get sign of value
sign=(n)-> n<0 and -1 or n>0 and 1 or 0

--check inbound index
chk=(id,d=0)->
	a=id%8+d%8
	b=id  +d
	0<=a and a<8 and 0<=b and b<64

--moves block
moves=
	rec:{-8,-1,1,8}
	dig:{-9,-7,7,9}
	all:{-9,-8,-7,-1,1,7,8,9}
	jmp:{-17,-15,-10,-6,6,10,15,17}
	
	--get moves for pawn
	pawn:(board,pos,team,moved)->
		r={}
		d=team and -8 or 8
		--test forward move
		if chk pos,d
			p=pos+d
			if board[p]==nil
				add r,p
				--if not moved yet
				if not moved and chk p,d
					p2=p+d
					add r,p2 if board[p2]==nil
		--test diagonal moves
		j=0
		for i=-1,1,2
			if chk p,i
				q=p+i
				o=board[q]
				if o~=nil --piece on the side
					add r,q if enm o,team
				elseif board.last~=nil --last move
					l=board.last[1] --last initial pos
					d=board.last[2] --last destination
					if(l<q and q<d)or(d<q and q<l)
						o=board[d]
						if o%8==1
							r["pass"..j]={q,d}
							j+=1 
		return r
	
	--get moves for linear pieces
	linear:(board,pos,team,dirs)->
		r={}
		for i=1,#dirs --for each direction
			d=dirs[i]
			p=pos
			while chk p,d
				p+=d
				o=board[p]
				--no other piece in the way
				if o==nil then add r,p
				else --other piece in the way
					add r,p if enm o,team
					break
				p+=d --next position
		return r
	
	--get moves for discrete pieces
	discrt:(board,pos,team,dirs)->
		r={}
		for i=1,#dirs --for each direction
			d=dirs[i]
			if chk pos,d
				p=pos+d
				o=board[p]
				--no other or enemy
				add r,p if o==nil or enm o,team
		return r
	
	--get moves for king
	king:(board,pos,team,moved)=>
		r=@.discrt board,pos,team,@all
		unless moved
			j=0
			for i=-4,3,7 --test castlings
				p=pos+i
				o=board[p]
				if o~=nil and o<16
					b=false  --path blocked?
					d=sign i --direction
					--for each cells between
					for q=pos,p,d --king and rook
						b=board[q]~=nil
						continue unless b
					unless b --path not blocked
						r["cast"..j]={pos+2*d,p,pos+j}
		return r
	
	--get moves of selected piece
	get:(brd,pos,tm,typ,moved)=>
		return switch typ
			when 1 then @.pawn   brd,pos,tm,moved
			when 4 then @.linear brd,pos,tm,@rec
			when 2 then @.discrt brd,pos,tm,@jmp
			when 3 then @.linear brd,pos,tm,@dig
			when 5 then @.linear brd,pos,tm,@all
			when 6 then @king    brd,pos,tm,moved
	
	--get moves plus king check
	getPlus:( board,pos,team,typ,moved)=>
		mvs=@get board,pos,team,typ,moved
		for k,v in pairs mvs
			B=copy board
			o=mvd B[pos] --get piece from board
			B[pos]=nil   --remove it from board
			--move is en passant or castling
			if type(k)=="string"
				B[v[1]]=o     --move king/pawn
				o=mvd B[v[2]] --get  rook/pawn
				B[v[2]]=nil   --remove from pos
				--if castling replace rook on board
				B[v[3]]=o if v[2]~=nil
			else B[v]=o
			--we try to find the king
			fKing=false
			for id,p in pairs B
				if type(id)=="number"and enm p,tm
					M=@get B,id,not tm,p%8,p>=16
					for l,w in pairs M
						fKing=type(l)=="number"and w%8==6
						break   if fKing
				break     if fKing
			mvs[k]=nil if fKing
		return mvs
	
	--get all moves for the turn
	getAll:(board,team)=>
		MVS={} --set of moves for each piece
		for p,t in pairs board
			if type(p)=="number"
				h=t>=16 --has moved
				t%=  16
				c=t>= 8 --team of piece
				t%=   8
				if team==c
					MVS[p]=@getPlus board,p,team,t,h
		return MVS


--game block
game=
	curs:27        --center of the board
	turn:true      --start with white
	board:mkBoard! --empty new board
	moves:nil      --set of moves for the turn 
	modif:false    --redraw board
	
	--init the turn
	initTurn:=>
		@turn =not @turn
		@moves=moves\getAll @board,@turn
	
	--update cursor from buttons
	cursorBTN:=>
		x,y=pos @curs
		x-=1 if btnp 2
		x+=1 if btnp 3
		y-=1 if btnp 1
		y+=1 if btnp 0
		if     x<0 then x=7
		elseif x>7 then x=0
		if     y<0 then y=7
		elseif y>7 then y=0
		c=x+8*y
		@modif=@curs~=c
		@curs=c
	
	cursorMS:=>
		x,y,pressed=mouse!
		x=( x-56)//16
		y=(132-y)//16
		if     x<0 then x=0
		elseif x>7 then x=7
		if     y<0 then y=0
		elseif y>7 then y=7
		c=x+8*y
		@modif=@curs~=c
		@curs=c

--draw block
draw=
	
	coords:(x,y)-> 56+x*16,116-y*16
	
	--draw the game
	game:(game)=>
		@board game.board
		x,y=pos game.curs
		@cursor x,y,game.turn
	
	--draw the goban with goishi's'
	board:(board)=>
		map 0,0,16,16,56,4
		for p,t in pairs board
			if type(p)=="number"
				x,y=pos p
				t%= 16
				c=t//8>=1
				t%=  8
				@piece x,y,c,t
	
	--draw the piece
	piece:(x,y,team,typ)=>
		x,y=@.coords x,y
		t=16+typ*2
		s=team and 2 or 0
		map t,s,2,2,x,y,11
	
	--draw the cursor
	cursor:(x,y,team)=>
		x,y=@.coords x,y
		t=18
		s=team and 6 or 4
		map t,s,2,2,x,y,11

cls 5
map 0,17,30,17
game\initTurn!
draw\game game

export TIC=->
	if useMouse then game\cursorMS!
	else             game\cursorBTN!
	draw\game game if game.modif
