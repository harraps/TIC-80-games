-- title:  tico tico
-- author: Olivier Schyns
-- desc:   puyo puyo like on TIC-80
-- script: moonscript

--puyo colors:
-- 1: red
-- 2: green
-- 3: blue
-- 4: yellow
-- 5: gray

--select the sprite to use
sprite=(T,L,C,R,B)->
	o,sp = C%2==1
	l,r  = (C==L),(C==R)
	if o then sp = 256+(C-1)*48
	else      sp = 296+(C-2)*48
	sp+= 2 if (C==B)
	sp+= 4 if (C==T)
	--left xor right
	if l~=r  then sp+= o and 8 or 24
	elseif l then sp+= 32 --left and right
	sp,(l and not r)

compact=(c,r)-> (c<<4)|r

--column of a game board
class Column
	new:=>
		@queue = {}
	puyo:(r)=> @queue[r]
	-- add puyo at the top
	add:   (p)=> table.insert @queue,p
	-- remove puyo at given row
	remove:(r)=> table.remove @queue,r
	--true if the column is filled
	filled:=> @queue[13]~=nil
	--number of puyo in the column
	nbPuyo:=> #@queue

--game board for a player
class Board
	new:=>
		@cols = {}
		@cols[i] = Column! for i=1,6
	
	--get puyo at given position
	puyo:(c,r)=>
		return nil if c<1 or 6<c
		@cols[c]\puyo r
	
	add:   (c,p)=> @cols[c]\add p
	remove:(c,r)=> @cols[c]\remove r
	
	--neighbors of the puyo at given pos
	neighbors:(c,r)=>@puyo(c,r+1),@puyo(c-1,r),@puyo(c,r),@puyo(c+1,r),@puyo(c,r-1)
	
	--group of puyo to delete
	group:(c,r)=>
		tp,gr = (@puyo c,r),{}
		@_group tp,gr,c,r if tp~=nil
		gr
		
	_group:(t,g,c,r)=>
		p = @puyo c,r
		if t==p
			m = compact c,r
			if not g[m]
				g[m] = true
				@_group t,g,c  ,r+1
				@_group t,g,c-1,r
				@_group t,g,c+1,r
				@_group t,g,c  ,r-1
	
	draw:(X)=>
		for i=1,6 --draw each column
			col = @cols[i]
			for j=1,col\nbPuyo!
				T,L,C,R,B = @neighbors i,j
				sp,l = sprite T,L,C,R,B
				f,x,y = 0,X+i*12,136-j*11
				f,x   = 1,x-4 if l
				spr sp,x,y,0,1,f,0,2,2

--block's head orientation:
-- 0: down
-- 1: right
-- 2: top
-- 3: left
--manage block of puyo
class Block
	new:(p1,p2,Brd)=>
		@p1,@p2,@Brd = p1,p2,Brd
		@x,@y,@o     = 3,13,0 --3rd column
	
	descend:=>
		if @y==math.floor @y
			fl = @o==2 and 2 or 1
			x2 = xhelp @x,@o
			p1 = @Brd\puyo @x,@y-fl
			p2 = @Brd\puyo x2,@y-1
			--puyo or floor below
			if p1~=nil or p2~=nil or @y<=fl
				a=true
				--destroy Block
				--put puyo in board
				--check combos
		@y-= 0.5
		return
	
	move:(m)=>
		if m~=0
			--compute the move
			y  = math.floor @y
			x1 = @x+m
			if     x1<1 then x1 = 1
			elseif x1>6 then x1 = 6
			--can we apply the move?
			x2 = @xhelp x1,@o
			p1 = @Brd\puyo x1,y
			p2 = @Brd\puyo x2,y
			--apply if space is free
			@x = x1 if p1==nil and p2==nil
		return
		
	rotate:(r)=>
		if r~=0
			--compute the rotation
			o = (@o+r+4)%4
			--can we apply the rotation?
			y1 = math.floor @y
			y2 = if o==2 and @y==y1
				@y-1 else -1 --> nil
			x  = @xhelp @x,o
			p1 = @Brd\puyo @x,y2
			p2 = @Brd\puyo  x,y1
			--apply if space is free
			if p1==nil and p2==nil
				@o = o
				@y = y1+1 if @o==2
		return
	
	xhelp:(x,o)->
		return switch o
			when 1 then x-1
			when 3 then x+1
			else -1 --> nil
	
	draw:(X)=>
		return
	
cls!

draw=(id,x,y,n)->
	a,b,c = 2,12, 8
	a,b,c = 2,28,24 if n
	spr id+a, x  ,y   , 0,1,0,0, 2,2
	spr id+b, x  ,y+11, 0,1,0,0, 2,2
	spr id+c, x+8,y+11, 0,1,1,0, 2,2
	return

